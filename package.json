  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "rm -f _next && cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env  BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint:js": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:style": "npx stylelint {components,containers,styles}/**/**/*.scss --ignore-path .gitignore",
    "lint:style:fix": "yarn lint:style -- --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "prepare": "husky install",
    "svgo": "npx svgo -q --folder UI/components/Svg/svgs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
