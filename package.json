{
  "name": "yellow-site-v2",
  "version": "2.0.0",
  "description": "Yellow website",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production NODE_PATH=./ next build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env  BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint:js": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:js:fix": "yarn lint:js -- --fix",
    "lint:style": "npx stylelint {components,containers,styles}/**/**/**/**/**/*.scss --syntax=scss --ignore-path .gitignore",
    "lint:style:fix": "yarn lint:style -- --fix",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.scss": [
      "yarn lint:style"
    ],
    "*.{js,jsx}": [
      "yarn lint:js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
