{
  "name": "yellow-site",
  "version": "2.0.0",
  "description": "Yellow website",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production NODE_PATH=./ next build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env  BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint:js": "eslint components/** containers/** hocs/** pages/** public/** redux/** server/** utils/** --ext .js,jsx --ignore-path .gitignore",
    "lint:js:fix": "yarn lint:js -- --fix",
    "lint:style": "npx stylelint {components,containers,styles}/**/**/**/**/**/*.scss --ignore-path .gitignore",
    "lint:style:fix": "yarn lint:style -- --fix",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.{js,jsx}": "yarn lint:js",
    "*.{scss}": "yarn lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
